<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cpu.quikdata">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".feature.QuikDataApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".feature.auth.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.evacuation.EvacuationActivity" />
        <activity android:name=".feature.createform.CreateFormActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cpu.quikdata.feature.main.MainActivity" />
        </activity>
        <activity android:name=".feature.main.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cpu.quikdata.feature.auth.AuthActivity" />
        </activity>
    </application>

</manifest>