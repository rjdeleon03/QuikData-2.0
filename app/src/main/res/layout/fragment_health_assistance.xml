<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.createform.healthinfo.healthassistance.HealthAssistanceFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/healthAssistanceRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        style="@style/RecyclerViewTheme"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/healthAssistanceAddButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimenMid"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/ic_add_24dp"/>

</RelativeLayout>