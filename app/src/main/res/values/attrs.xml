<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DividerText">
        <attr name="dividerText" format="string" />
    </declare-styleable>

    <declare-styleable name="CollapsibleContainer">
        <attr name="headerText" format="string" />
        <attr name="collapsed" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextItem">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="ItemSection" parent="TextItem">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="Question" parent="TextItem">
        <attr name="question" format="string" />
    </declare-styleable>

    <declare-styleable name="StringQuestion" parent="Question">
        <attr name="textHint" format="string" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="NumberQuestion" parent="StringQuestion">
        <attr name="textHint" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="BooleanQuestion" parent="Question">
        <attr name="question" />
        <attr name="yesText" format="string" />
        <attr name="noText" format="string" />
    </declare-styleable>

    <declare-styleable name="MultilineStringQuestion" parent="StringQuestion">
        <attr name="textHint" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="MultilineStringLongQuestion" parent="MultilineStringQuestion">
        <attr name="textHint" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="BooleanMultilineStringLongQuestion" parent="BooleanQuestion">
        <attr name="questionBoolean" format="string" />
        <attr name="questionString" format="string"/>
        <attr name="textString" format="string" />
        <attr name="yesText" />
        <attr name="noText" />
    </declare-styleable>

    <declare-styleable name="DateQuestion" parent="Question">
        <attr name="question" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="TwoNumbersQuestion" parent="Question">
        <attr name="question" />
        <attr name="textHint1" format="string" />
        <attr name="text1" format="string" />
        <attr name="textHint2" format="string" />
        <attr name="text2" format="string" />
    </declare-styleable>

</resources>